@article{Mendoza:wr,
author = {Mendoza, Martha and Alegr{\'\i}a, Erwin and Maca, Manuel and Cobos, Carlos and Le{\'o}n, Elizabeth},
title = {{Multidimensional analysis model for a document warehouse that includes textual measures}},
journal = {Decision Support Systems},
volume = {72 IS -},
pages = {44--59},
rating = {0},
date-added = {2017-11-10T11:52:44GMT},
date-modified = {2018-06-19T12:09:39GMT},
abstract = {Abstract Data warehouses and On-Line Analytical Processing tools, OLAP, together permit a multi-dimensional analysis of structured data information. However, as business systems are increasingly required to handle substantial quantities of unstructured textual information, the need arises for an effective and similar means of analysis. To manage unstructured text data stored in data warehouses, a new multi-dimensional analysis model is proposed that includes textual measures as well as a topic hierarchy. In this model, the textual measures that associate the topics with the text documents are generated by Probabilistic Latent Semantic Analysis, while the hierarchy is created automatically using a clustering algorithm. Documents are then able to be queried using OLAP tools. The model was evaluated from two viewpoints {\textemdash} query execution time and user satisfaction. Evaluation of execution time was carried out on scientific articles using two query types and user satisfaction (with query time and ease of use) using statistical frequency and multivariate analyses. Encouraging observations included that as the number of documents increases, query time increases as a lineal, rather than exponential tendency. In addition, the model gained an increasing acceptance with use, while the visualization of the model was also well received by users.},
url = {https://www.sciencedirect.com/science/article/pii/S0167923615000287},
uri = {\url{papers3://publication/uuid/0A3F6FE5-8E13-4DF1-8FBC-823193E0E446}}
}


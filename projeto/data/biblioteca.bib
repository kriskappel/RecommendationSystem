@article{Nasiri:2017ui,
author = {Nasiri, Sara and Zenkert, Johannes and Fathi, Madjid},
title = {{Improving CBR adaptation for recommendation of associated references in a knowledge-based learning assistant system}},
journal = {Applications in Computational Intelligence (Selected an improved papers of the 13th International Work-Conference on Artificial Neural Networks, IWANN2015)},
year = {2017},
volume = {250 IS -},
pages = {5--17},
month = aug,

@article{Rader:ul,
author = {Rader, Emilee},
title = {{Examining user surprise as a symptom of algorithmic filtering}},
journal = {International Journal of Human Computer Studies},
volume = {98 IS -},
pages = {72--88},
rating = {0},
date-added = {2017-11-10T11:52:30GMT},
date-modified = {2018-06-19T12:10:06GMT},
abstract = {Abstract The Facebook News Feed prioritizes posts for display by ranking them more prominently in the News Feed, based on users{\textquoteright} past interactions with the system. This study investigated constraints imposed on social interactions by the algorithm, by triggering participants{\textquoteright} awareness of {\textquotedblleft}missed posts{\textquotedblright} in their Friends{\textquoteright} Timelines that they did not remember seeing before. If the algorithm prioritizes posts from people that users feel closer to and want to stay in touch with, participants should be less likely to report missed posts from close Friends. However, the results showed that relationship closeness had no effect on the likelihood of noticing a missed post, after controlling for how many Facebook Friends participants had and the accuracy of participants{\textquoteright} memories for their Friends{\textquoteright} Facebook activity. Also, missed posts from close Friends were more surprising, even when participants believed that the actions of the system caused the missed posts, indicating that these instances represent participants{\textquoteright} unmet expectations for the behavior of their News Feeds. Because Facebook posts present opportunities for feedback important for social support and maintaining social ties, this could indicate bias in the way the algorithm promotes content that could affect users{\textquoteright} ability to maintain relationships on Facebook. These findings have implications for approaches to improve user control and increase transparency in systems that use algorithmic filtering.},
url = {https://www.sciencedirect.com/science/article/pii/S1071581916301380},
uri = {\url{papers3://publication/uuid/4EBBE469-239E-445A-9E16-43BE8DA455E3}}
}

@article{Manca:vz,

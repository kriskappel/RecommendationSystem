@article{ArnauGonzalez:vc,
author = {Arnau-Gonz{\'a}lez, Pablo and Arevalillo-Herr{\'a}ez, Miguel and Ramzan, Naeem},
title = {{Fusing highly dimensional energy and connectivity features to identify affective states from EEG signals}},
journal = {Applications in Computational Intelligence (Selected an improved papers of the 13th International Work-Conference on Artificial Neural Networks, IWANN2015)},
volume = {244 IS -},
pages = {81--89},
rating = {0},
date-added = {2017-11-10T11:52:50GMT},
date-modified = {2018-06-19T12:10:06GMT},
abstract = {Abstract In this paper, a novel method for affect detection is presented. The method combines both connectivity-based and channel-based features with a selection method that considerably reduces the dimensionality of the data and allows for an efficient classification. In particular, the Relative Energy (RE) and its logarithm in the spacial domain, and the Spectral Power (SP) in the frequency domain are computed for the four typical frequency bands ($\alpha$, $\beta$, $\gamma$ and $\theta$), and complemented with the Mutual Information measured over all channel pairs. The resulting features are then reduced by using a hybrid method that combines supervised and unsupervised feature selection. First, Welch{\textquoteright}s t-test is used to select the features that best separate the classes, and discard the ones that are less useful for classification. To this end, all features where the t-test yields a p-value above a threshold are eliminated. The remaining ones are further reduced by using Principal Component Analysis. Detection results are compared to state-of-the-art methods on DEAP, a database for emotion analysis composed of labeled recordings from 32 subjects while watching 40 music videos. The effect of using different classifiers is also evaluated, and a significant improvement is observed in all cases.},
url = {https://www.sciencedirect.com/science/article/pii/S0925231217305222},
uri = {\url{papers3://publication/uuid/065D1D03-8502-41A6-B105-0E36E3E89BD1}}
}


@article{Nilashi:ux,
author = {Nilashi, Mehrbakhsh and Ahmadi, Hossein and Ahani, Ali and Ravangard, Ramin and Ibrahim, Othman bin},
title = {{Determining the importance of Hospital Information System adoption factors using Fuzzy Analytic Network Process (ANP)}},
journal = {Technological Forecasting and Social Change},

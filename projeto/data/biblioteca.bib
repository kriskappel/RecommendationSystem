@article{Wang:wj,
author = {Wang, Qi and Ma, Jian and Liao, Xiuwu and Du, Wei},
title = {{A context-aware researcher recommendation system for university-industry collaboration on R{\&}amp;D projects}},
journal = {Decision Support Systems},
volume = {103 IS -},
pages = {46--57},
rating = {0},
date-added = {2017-11-10T11:52:27GMT},
date-modified = {2018-06-19T12:09:56GMT},
abstract = {Abstract University-industry collaboration plays an important role in the success of R{\&}amp;D projects. One of the main challenges of university-industry collaboration is the identification of suitable partners. Due to the information asymmetry problem, it is difficult for companies to identify researchers from universities for collaboration on their R{\&}amp;D projects. Various expert recommendation systems (e.g., question responder recommenders and co-author recommenders) have been proposed, but they fail to characterize companies' needs in identifying suitable researchers. This paper proposes a context-aware researcher recommendation system to encourage university-industry collaboration on industrial R{\&}amp;D projects. The system has two modules: an offline preparation module and an online recommendation module. In the offline preparation module, candidate researchers are identified in advance to improve the efficiency of the context-aware recommendation. In the online recommendation module, contextual information (i.e., R{\&}amp;D projects) is captured from a social network platform, and then, candidate researchers are recommended based on a contextual trust analysis model, which combines the expertise relevance, quality, and trust relations of researchers to profile and evaluate candidate researchers for the R{\&}amp;D project collaboration. An offline experiment and a user study are conducted to evaluate the effectiveness of the proposed recommendation system. The results show that the proposed method achieves better performance than the baseline methods.},
url = {https://www.sciencedirect.com/science/article/pii/S0167923617301562},
uri = {\url{papers3://publication/uuid/2430108F-CB22-4D27-9907-2526C0437127}}
}

@inproceedings{Sun:ii,
